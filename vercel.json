{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "/dashboard.html"
    },
    {
      "src": "/app",
      "dest": "/dashboard.html"
    },
    {
      "src": "/app/(.*)",
      "dest": "/dashboard.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/signup",
      "dest": "/signup.html"
    },
    {
      "src": "/forgot-password",
      "dest": "/forgot-password.html"
    },
    {
      "src": "/(meetings|scheduling|availability|contacts|workflows|integrations|routing|landing-page|analytics|admin|account)(/.*)?",
      "dest": "/dashboard.html"
    },
    {
      "src": "/(pricing|product|solutions|resources|about|contact|help-center|developers|community|blog|case-studies|security|careers|press|terms|privacy|legal|status|cookie-settings)",
      "dest": "/index.html"
    },
    {
      "src": "/([^/]+)/([^/]+)",
      "dest": "/booking.html"
    },
    {
      "src": "/([^/.]+)",
      "dest": "/public-landing.html"
    }
  ]
}